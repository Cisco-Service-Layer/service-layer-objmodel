{
  "register_v4_vrfs" : {
    "purge_interval": 300,
    "admin_distance": 2,
    "vrf_list": ["vxlan_red", "vxlan_blue"]
  },
  "register_v6_vrfs" : {
    "purge_interval": 300,
    "admin_distance": 2,
    "vrf_list": ["vxlan_red", "vxlan_blue"]
  },
  "program_v4_vxlan_routes" : {
    "batch_size" : 2,
    "prefix_vrf" : "vxlan_red",
    "routes" : [
    {
      "prefix": "172.16.30.0",
      "prefix_len": 24,
      "paths": [
      {
        "path_vrf": "default",
        "single_path": true,
        "encap_type": "encap_vxlan",
        "encap_afi": 4,
        "vni": 94011,
        "v4_src_ip": "120.10.20.1",
        "v4_dst_ip": "100.10.30.2",
        "vxlan_src_mac": "CC:1B:77:49:54:FD",
        "vxlan_dst_mac": "00:00:00:00:00:00"
      }]
    },
    {
      "prefix": "172.16.40.0",
      "prefix_len": 24,
      "paths": [
      {
        "path_vrf": "default",
        "single_path": true,
        "encap_type": "encap_vxlan",
        "encap_afi": 6,
        "vni": 96011,
        "v6_src_ip": "120:10:20::1",
        "v6_dst_ip": "100:10:30::2",
        "vxlan_src_mac": "CC:1B:77:49:56:FD",
        "vxlan_dst_mac": "00:00:00:00:00:00"
      }]
    }]
  }
}

